<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
    <!--
        Added the constraints for entity Questions.
    -->
    <changeSet id="20211019160320-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="question_id"
                                 baseTableName="questions"
                                 constraintName="fk_questions__question_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question_answer"/>

        <addForeignKeyConstraint baseColumnNames="form_id"
                                 baseTableName="questions"
                                 constraintName="fk_questions__form_id"
                                 referencedColumnNames="id"
                                 referencedTableName="form"/>

        <addForeignKeyConstraint baseColumnNames="question_option_id"
                                 baseTableName="questions"
                                 constraintName="fk_questions__question_option_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question_option"/>
    </changeSet>
</databaseChangeLog>
